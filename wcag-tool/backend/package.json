  "main": "src/server.js",
  "private": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development node src/server.js",
    "dev": "cross-env NODE_ENV=development nodemon src/server.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "cross-env NODE_ENV=test mocha --timeout 10000 --recursive --exit --require ./env tests/unit",
    "test:integration": "cross-env NODE_ENV=test mocha --timeout 10000 --recursive --exit --require ./env tests/integration",
    "lint": "eslint --ext .js .",
    "lint:fix": "npm run lint -- --fix",
    "seed": "cross-env NODE_ENV=development node --require ./env -e 'require(\"./src/utils/seed.js\").main()'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.24.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "html": "^1.0.0",
    "mongoose": "^6.0.13",
    "multer": "^1.4.3",
    "multer-gridfs-storage": "^5.0.2",
    "sanitize-html": "^2.6.1",
    "ws": "^8.3.0",
    "y-websocket": "^1.3.18"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.1",
    "mocha": "^9.1.3",
    "mongodb-memory-server": "^8.0.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1"
  }
}
